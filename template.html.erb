<html>
  <head>
    <link rel="stylesheet" href="style.css">
    <!-- etc... -->
  </head>
  <body>
    <div class='summary'>
      <% @expense_summary.each do |expense_month| %>
        <h1>
          <%= expense_month.keys[0] %>
        </h1>
        <div class="monthSummary" id=<%= expense_month.keys[0]%>>
          <ul>
            <% expense_month.values[0].each do |expense| %>
              <li>
                Desc: <%= expense.keys[0] %> </br>
                Amount: <%= expense.values[0] %>
              </li>
            <% end %>
          </ul>
        </div>
      <% end %>
    </div>
    <script src="./main.js"></script>
  </body>
</html>